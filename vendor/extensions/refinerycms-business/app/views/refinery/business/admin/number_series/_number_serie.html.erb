<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(number_serie) -%>">
  <span class='title'>
    <%= number_serie.identifier %>
    <span class="preview">
      <%= t('.preview') %>
    </span>
  </span>
  <span class='actions'>
    <%= action_icon(:edit,    refinery.edit_business_admin_number_series_path(number_serie), t('.edit') ) %>
    <%= action_icon(:delete,  refinery.business_admin_number_series_path(number_serie), t('.delete'),
                    { class: "cancel confirm-delete",
                      data: {confirm: t('message',  scope: 'refinery.admin.delete', title: number_serie.identifier)}}  ) %>
  </span>
</li>
