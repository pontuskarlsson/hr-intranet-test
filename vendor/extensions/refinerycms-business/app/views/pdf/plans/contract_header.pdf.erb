<% content_for :header_summary do %>
  <table class="document-summary-table">
    <thead>
    <tr>
      <th>Reference</th>
      <th>Title</th>
      <th>Status</th>
      <th>Start Date</th>
      <th>Your Reference</th>
      <th>Account Manager</th>
    </tr>
    </thead>
    <tbody>
    <tr>
      <td><%= @plan.reference %></td>
      <td><%= @plan.title %></td>
      <td><%= @plan.display_status %></td>
      <td><%= @plan.display_start_date %></td>
      <td><%= @plan.contact_person_full_name %></td>
      <td><%= @plan.account_manager_full_name %></td>
    </tr>
    </tbody>
  </table>
<% end %>
