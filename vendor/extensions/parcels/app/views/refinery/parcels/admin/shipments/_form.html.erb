<%= form_for [refinery, :parcels_admin, @shipment] do |f| -%>
  <%= render '/refinery/admin/error_messages',
              :object => @shipment,
              :include_object_name => true %>



  <%= render '/refinery/admin/form_actions', :f => f,
             :continue_editing => false,
             :delete_title => t('delete', :scope => 'refinery.parcels.admin.shipments.shipment'),
             :delete_confirmation => t('message', :scope => 'refinery.admin.delete', :title => @shipment.to_address.try(:name)) -%>
<% end -%>
