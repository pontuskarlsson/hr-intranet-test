<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(parcel) -%>">
  <span class='title'>
    <% if parcel.air_waybill_no.present? %>
      <%= parcel.from_name %>
    <% else %>
      <span class="italic">N/A</span>
    <% end %>
    <span class="preview">
      <%= t('.preview', from_name: parcel.from_name, to_name: parcel.to_name, :parcel_date => l(parcel.parcel_date, :format => :short)) %>
    </span>
  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.shipping_parcel_path(parcel),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_shipping_admin_parcel_path(parcel),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.shipping_admin_parcel_path(parcel),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => parcel.from_name),
        :method => :delete %>
  </span>
</li>
