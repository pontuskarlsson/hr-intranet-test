<%= render 'refinery/admin/search_header', :url => refinery.shipping_admin_routes_path %>
<div class='pagination_container'>
  <% if @routes.any? %>
    <%= render 'routes' %>
  <% else %>
    <p>
      <% unless searching? %>
        <strong>
          <%= t('.no_routes_yet') %>
        </strong>
      <% else %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% end %>
    </p>
  <% end %>
</div>
