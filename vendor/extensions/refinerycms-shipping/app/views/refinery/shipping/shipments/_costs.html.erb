    <th>Terminal Fee</th>
    <th>Domestic Transportation Cost</th>
    <th>Forwarding Fee</th>
    <th>Freight Cost</th>
  </tr>
  </thead>
  <tbody>
  <tr>
    <td><%= shipment.rate_currency %></td>
