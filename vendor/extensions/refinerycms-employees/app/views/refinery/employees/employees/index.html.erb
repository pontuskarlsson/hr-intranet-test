<% content_for :body do %>
  <h1>Employee Directory</h1>
  <div class="employees">

    <ul class="medium-block-grid-3">
      <% @employees.each do |employee| %>
        <li>
          <%= render '/refinery/employees/employees/preview', employee: employee %>
        </li>
      <% end %>
    </ul>

  </div>
<% end %>

<%= render '/refinery/content_page' %>
